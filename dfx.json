      "candid": "src/nexBit_backend/nexBit_backend.did",
      "gzip": true,
      "post_install": ".kybra/nexBit_backend/post_install.sh",
      "tech_stack": {
        "cdk": {
          "kybra": {}
        },
        "language": {
          "python": {}
        }
      },
      "type": "custom",
      "wasm": ".kybra/nexBit_backend/nexBit_backend.wasm"
    },
    "nexBit_frontend": {
      "dependencies": [
        "nexBit_backend"
